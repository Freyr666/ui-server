{
    "pipeline": {
        "bin_name": "ats3-backend",
        "bin_path": "../ats-analyzer/build/",
        "msg_fmt": "json",
        "sock_in": "ipc:///tmp/ats_qoe_in",
        "sock_out": "ipc:///tmp/ats_qoe_out"
    },
    "network": {
        "extern" : null,
        "intern" : {
            "interface": "eno1",
            "address": ["192.168.111.237", 24],
            "routes": { "gateway" : "192.168.111.1",
                        "static"  : [] },
            "mac_address": null
        }
    },
    "db": {
        "socket_path": "/tmp", "cleanup":48, "password":"ats"
    },
    "config_storage": {
        "config_dir": "/tmp/"
    },
    "topology":
    [ "CPU",
      { "process": "pipeline",
        "ifaces":
        [ { "iface": "eth0", "conn":
            [ "Board",
              { "typ": "TS2IP",
                "control": 0,
                "model": "ts2ip",
                "manufacturer": "niitv",
                "version": 1,
                "ports": [ {"port":1,
                            "child":
                            [ "Board",
                              { "typ": "TS",
                                "control": 2,
                                "model": "qos",
                                "manufacturer": "niitv",
                                "version": 1,
                                "sources": { "input": 1,
                                             "t2mi": 2
                                           },
                                "ports": [{"port": 0,
                                           "switchable": true,
                                           "child":
                                           [ "Board",
                                             { "typ": "IP2TS",
                                               "control": 5,
                                               "model": "dtm-3200",
                                               "manufacturer": "dektec",
                                               "version": 1,
                                               "ports": [ {"port": 0,
                                                           "child": [ "Input",
                                                                      { "input":"TSOIP",
                                                                        "id": 1
                                                                      }]
                                                          }]
                                             }]
                                          },
                                          {"port": 1,
                                           "switchable": true,
                                           "child": [ "Input",
                                                      { "input": "ASI",
                                                        "id": 1
                                                      }]
                                          }]
                              }]
                           },
                           {"port":0,
                            "child":
                            [ "Board",
                              {"typ": "TS",
                               "control": 1,
                               "model": "qos",
                               "manufacturer": "niitv",
                               "version": 1,
                               "sources": { "input": 3,
                                            "t2mi": 4
                                          },
                               "ports": [{"port": 0,
                                          "switchable": true,
                                          "child":
                                          ["Board",
                                           {"typ":"DVB",
                                            "control": 4,
                                            "model": "rf",
                                            "manufacturer": "niitv",
                                            "version": 1,
                                            "sources": 5,
                                            "ports": [{"port": 0,
                                                       "child": [ "Input",
                                                                  { "input":"RF",
                                                                    "id": 1
                                                                  }]
                                                      }]
                                           }]
                                         },
                                         {"port": 1,
                                          "switchable": true,
                                          "child": [ "Input",
                                                     { "input": "ASI",
                                                       "id": 2
                                                     }]
                                         }]
                              }]
                           }]
              }]
          }]
      }],
    "server": {
        "path": "./resources",
        "port": 8080
    }
}

